loadStartingId(String ref) {
    String idQ = "SELECT value FROM localSettings WHERE key = '" + ref + "';";
    fetchOne(idQ, new FetchCallback() {
        onFetch(result) {
            if (!isNull(result)) {
                setFieldValue(ref, result.get(0));
            } else {
                setFieldValue(ref, "1");
            }
        }
    });
}

