<ValidationSchema>
 
<ArchaeologicalElement name='Context'>
<property name='AreaCode'>
    <validator type='blankchecker'>
        <param type='field' value='vocab' />
    </validator>
</property>
<property name='Context ID'>
    <validator type='blankchecker'>
        <param type='field' value='measure' />
    </validator>
</property>
<property name='Context Name'>
    <validator type='blankchecker'>
        <param type='field' value='measure' />
    </validator>
</property>
